<hibernate-mapping>
    <class name="rzaharov.carlist.models.Condition" table="condition">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="condition" column="condition"/>
        <property name="year" column="year"/>
        <property name="mileage" column="mileage"/>

        <many-to-one name="car"
                     column="id_car"
                     class="rzaharov.carlist.models.Car"/>
    </class>
</hibernate-mapping>
