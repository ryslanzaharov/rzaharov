<hibernate-mapping>
    <class name="rzaharov.carlist.models.Engine" table="engine">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="name" column="name"/>
        <property name="type_engine" column="type_engine"/>
        <property name="condition" column="condition"/>

        <bag name="cars" inverse="true">
            <key column="engine_id" not-null="true"/>
            <one-to-many class="rzaharov.carlist.models.Car"/>
        </bag>
    </class>
</hibernate-mapping>